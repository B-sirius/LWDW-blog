{
  "name": "banana-workshop",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "yarn updateMap && next dev",
    "build": "yarn updateMap && next build",
    "lint": "next lint",
    "updateMap": "ts-node server/updateMap.ts && ts-node server/generateRSSFeed.ts",
    "forceUpdateMap": "rm -f postMap.json && yarn updateMap",
    "export": "yarn build && next export -o docs/ && touch ./docs/.nojekyll && ts-node server/generateBlogGist.ts",
    "startDoc": "http-server ./docs",
    "analyze": "ANALYZE=true yarn build"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@next/bundle-analyzer": "^13.1.1",
    "dayjs": "^1.11.5",
    "next": "^13.1.1",
    "prism-themes": "^1.9.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "rehype": "^12.0.1",
    "rehype-external-links": "^2.0.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-angular": "^17.6.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.7.23",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "8.24.0",
    "eslint-config-next": "^13.1.1",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "rss": "^1.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
